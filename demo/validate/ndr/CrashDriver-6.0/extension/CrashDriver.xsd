<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
           xmlns:ct="http://release.niem.gov/niem/conformanceTargets/3.0/"
           xmlns:exch="http://training.niem.gov/CrashDriver/1.1/"
           xmlns:ext="http://training.niem.gov/CrashDriver/1.1/extension"
           xmlns:j="https://docs.oasis-open.org/niemopen/ns/model/domains/justice/6.0/"
           xmlns:nc="https://docs.oasis-open.org/niemopen/ns/model/niem-core/6.0/"
           xmlns:structures="http://release.niem.gov/niem/structures/5.0/"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           ct:conformanceTargets="https://docs.oasis-open.org/niemopen/ns/specification/XNDR/6.0/#ExtensionSchemaDocument"
           xml:lang="en-US"
           targetNamespace="http://training.niem.gov/CrashDriver/1.1/"
           version="1">
   <xs:annotation>
      <xs:documentation>Exchange schema for an example IEPD used to illustrate NIEM's technical
      features.</xs:documentation>
   </xs:annotation>
   <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/structures/6.0/" schemaLocation="../utility/structures.xsd"/>
   <xs:import namespace="http://training.niem.gov/CrashDriver/1.1/extension" schemaLocation="extension.xsd"/>
   <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/domains/justice/6.0/" schemaLocation="../niem/domains/jxdm.xsd"/>
   <xs:import namespace="https://docs.oasis-open.org/niemopen/ns/model/niem-core/6.0/" schemaLocation="../niem/niem-core.xsd"/>
   <xs:complexType name="CrashDriverInfoType">
      <xs:annotation>
         <xs:documentation>A data type for a collection of information about the driver of a vehicle in a crash.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="structures:ObjectType">
            <xs:sequence>
               <xs:element ref="nc:Person" maxOccurs="unbounded"/>
               <xs:element ref="j:Crash"/>
               <xs:element ref="j:PersonChargeAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xs:element ref="j:Charge" minOccurs="0" maxOccurs="unbounded"/>
               <xs:element ref="j:Metadata" minOccurs="0"/>
               <xs:element ref="ext:PrivacyMetadata" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:element name="CrashDriverInfo" type="exch:CrashDriverInfoType">
      <xs:annotation>
         <xs:documentation>A collection of information about the driver of a vehicle in a crash.</xs:documentation>
      </xs:annotation>
   </xs:element>
</xs:schema>
